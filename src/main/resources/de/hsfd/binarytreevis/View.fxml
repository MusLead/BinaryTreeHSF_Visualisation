         <DropShadow/>
      </effect>
      <cursor>
         <Cursor fx:constant="HAND"/>
      </cursor>
   </Button>
   <Button fx:id="avl" layoutX="169.0" layoutY="250.0" mnemonicParsing="false" prefHeight="44.0"
           prefWidth="77.0" style="-fx-background-color: #F0E68C;" text="AVL">
      <effect>
         <DropShadow/>
      </effect>
      <cursor>
         <Cursor fx:constant="HAND"/>
      </cursor>
   </Button>
   <Button fx:id="rbt" layoutX="312.0" layoutY="250.0" mnemonicParsing="false" prefHeight="44.0"
           prefWidth="77.0" style="-fx-background-color: #CD5C5C;" text="RBT">
      <effect>
         <DropShadow/>
      </effect>
      <cursor>
         <Cursor fx:constant="HAND"/>
      </cursor>
   </Button>

   <Label layoutX="140.0" layoutY="310.0" text="©️ 2018 Ankit Sharma"
          style="-fx-font-size: 12px; -fx-text-fill: black;"/>
</AnchorPane>
